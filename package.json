	"scripts": {
		"build": "./node_modules/.bin/grunt",
		"clean": "rm -rf docs && rm -rf buildresults && touch ./logs/init.log && rm -f ./logs/*.log",
		"debug": "node --inspect-brk ./server/index.js",
		"docs": "./node_modules/.bin/jsdoc --verbose --configure ./jsdoc.json",
		"lint": "./node_modules/.bin/eslint .",
		"start": "touch ./logs/init.log && rm -f ./logs/*.log && node ./server/index.js",
		"dev-start": "touch ./logs/init.log && rm -f ./logs/*.log && BLUEBIRD_DEBUG=1 DEBUG=* node ./server/index.js",
		"test": "NODE_ENV=test ./node_modules/.bin/mocha --forbid-only \"test/**/*.spec.js\"",
		"clinic-doctor": "NODE_ENV=test ./node_modules/.bin/clinic doctor --collect-only --on-port=\"sleep 20 && ./node_modules/.bin/autocannon -c100 localhost:9100/\" -- node ./server/index.js",
		"clinic-flame": "NODE_ENV=test ./node_modules/.bin/clinic flame --collect-only --on-port=\"sleep 20 && ./node_modules/.bin/autocannon -c100 localhost:9100/\" -- node ./server/index.js",
		"clinic-profile": "NODE_ENV=test ./node_modules/.bin/clinic bubbleprof --collect-only --on-port=\"sleep 20 && ./node_modules/.bin/autocannon -c100 localhost:9100/\" -- node ./server/index.js",
		"clinic-upload": "./node_modules/.bin/clinic upload *.clinic-doctor && ./node_modules/.bin/clinic upload *.clinic-bubbleprof",
		"clinic-clean": "NODE_ENV=test ./node_modules/.bin/clinic clean"
	},
